/*ARDUINO PLAY MELODY*/ //LAB 6int sensorValue;int sensorLow = 1023;int sensorHigh = 0;const int pin = 8;void setup() {     //LAB 6    pinMode(pin, OUTPUT);    digitalWrite(pin, HIGH);        while(millis() < 5000) {      sensorValue = analogRead(A0);      if (sensorValue > sensorHigh) {        sensorHigh = sensorValue;      }      if (sensorValue < sensorLow) {        sensorLow = sensorValue;      }    }  digitalWrite (pin, LOW);       }void loop() {     //LAB 6     sensorValue = analogRead(A0);        int pitch = map(sensorValue, sensorLow, sensorHigh, 50, 4000);    tone(8, pitch, 20);            delay(10);}